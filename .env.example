# Environment variables for ZenLive Server
# Copy this file to .env and fill in your values

# API Keys (auto-generated if not provided)
ZENLIVE_API_KEY=
ZENLIVE_SECRET_KEY=

# Redis (optional, for scaling)
REDIS_URL=redis:6379
REDIS_PASSWORD=

# Server ports
HTTP_PORT=7880
RTC_PORT=7881

# Development mode
DEV_MODE=false
